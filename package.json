{
  "name": "antblog",
  "version": "1.0.0",
  "description": "RESTful API service for ifuyun.com blog",
  "author": {
    "name": "Fuyun",
    "url": "http://www.ifuyun.com/"
  },
  "license": "MIT",
  "homepage": "https://github.com/ifuyun/antblog#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ifuyun/antblog.git"
  },
  "bugs": {
    "url": "https://github.com/ifuyun/antblog/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env ENV=development nest start",
    "start:dev": "cross-env ENV=development nest start --watch",
    "start:debug": "cross-env ENV=development nest start --debug --watch",
    "start:prod": "cross-env ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^1.2.1",
    "@nestjs/core": "^8.4.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/sequelize": "^8.0.0",
    "async": "^3.2.3",
    "body-parser": "^1.19.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "crypto": "^1.0.1",
    "csurf": "^1.11.0",
    "ejs": "^3.1.7",
    "express-session": "^1.17.2",
    "formidable": "^2.0.1",
    "gm": "^1.23.1",
    "helmet": "^5.0.2",
    "lodash": "^4.17.21",
    "log4js": "^6.4.6",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.3",
    "mysql2": "^2.3.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "redis": "^4.0.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "sanitizer": "^0.1.3",
    "sequelize": "^6.19.0",
    "sequelize-typescript": "^2.1.3",
    "serve-favicon": "^2.5.0",
    "svg-captcha": "^1.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.10",
    "@nestjs/testing": "^8.4.4",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/formidable": "^2.0.4",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/log4js": "^2.3.5",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.30",
    "@types/passport-jwt": "^3.0.6",
    "@types/sanitizer": "^0.0.28",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "keywords": [
    "Blog",
    "CMS",
    "Node.js",
    "Nest",
    "MySQL",
    "Sequelize",
    "Redis",
    "Fuyun"
  ]
}
